# Cache and logs (Symfony2)
Symfony/app/cache/*
Symfony/app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Email spool folder
Symfony/app/spool/*

# Cache, session files and logs (Symfony3)
Symfony/var/cache/*
Symfony/var/logs/*
Symfony/var/sessions/*
!var/cache/.gitkeep
!var/logs/.gitkeep
!var/sessions/.gitkeep

# Parameters
Symfony/app/config/parameters.yml
Symfony/app/config/parameters.ini

# Managed by Composer
Symfony/app/bootstrap.php.cache
Symfony/var/bootstrap.php.cache
Symfony/bin/*
!bin/console
!bin/symfony_requirements
Symfony/vendor/

# Assets and user uploads
Symfony/web/bundles/
Symfony/web/uploads/

# Assets managed by Bower
Symfony/web/assets/vendor/

# PHPUnit
Symfony/app/phpunit.xml
Symfony/phpunit.xml

# Build data
Symfony/build/

# Composer PHAR
Symfony/composer.phar

# Backup entities generated with doctrine:generate:entities command
*/Entity/*~

# Other
/Data/
